{
  "name": "nestjs-uws",
  "version": "1.0.6",
  "description": "NestJS UWebSocket.js adapter",
  "author": "Vladislav Len <len.vladislavs@gmail.com>",
  "license": "MIT",
  "readmeFilename": "README.md",
  "main": "dist/",
  "files": [
    "dist/**/*",
    "*.md"
  ],
  "scripts": {
    "start:dev": "tsc -w",
    "build": "npx tsc",
    "prepare": "npm run build",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "keywords": [
    "nestjs",
    "websockets",
    "uwebsocket"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/heavenlyteam/nestjs-uws"
  },
  "peerDependencies": {
    "@nestjs/common": "^7.6.13"
  },
  "dependencies": {
    "@nestjs/websockets": "^7.6.13",
    "rxjs": "^6.6.6",
    "uWebSockets.js": "github:uNetworking/uWebSockets.js#v18.3.0"
  },
  "devDependencies": {
    "@nestjs/common": "^7.6.13",
    "@nestjs/core": "^7.6.13",
    "@nestjs/platform-express": "^7.6.13",
    "@nestjs/schematics": "^7.2.8",
    "@nestjs/testing": "^7.6.13",
    "@types/express": "^4.17.11",
    "@types/jest": "26.0.20",
    "@types/node": "^14.14.31",
    "@types/supertest": "2.0.10",
    "jest": "26.6.3",
    "prettier": "2.2.1",
    "supertest": "6.1.3",
    "ts-jest": "26.5.3",
    "ts-loader": "^8.0.17",
    "ts-node": "^9.1.1",
    "tsc-watch": "4.2.9",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.2.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
